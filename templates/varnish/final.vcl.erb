vcl 4.0;
# generated varnish config: <%= ENVIRONMENT %>
# local test: <%= LOCAL_TEST_MODE ? 'ENABLED' : 'DISABLED' %>

### If nothing special is done by any plugin we receive the page from the core plugin
### Will render 404 if nothing is available
sub vcl_recv {
	set req.backend_hint = core_backend_director.backend();
	set req.url = "/template/page?req_url=" + req.url + "&req_host=" + req.http.host;
	return (hash);
}
